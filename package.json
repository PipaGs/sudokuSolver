{
  "name": "@pipags/sudoku-solver",
  "version": "1.1.0",
  "description": "Sudoku solver and generator with TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sudoku": {
      "types": "./dist/sudoku.d.ts",
      "import": "./dist/sudoku.js"
    },
    "./generator": {
      "types": "./dist/generator.d.ts",
      "import": "./dist/generator.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "example": "ts-node src/examples.ts",
    "lint": "eslint . --ext .ts,.js"
  },
  "keywords": [
    "sudoku",
    "puzzle",
    "generator",
    "solver",
    "typescript"
  ],
  "author": "PipaGs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PipaGs/sudokuSolver.git"
  },
  "bugs": {
    "url": "https://github.com/PipaGs/sudokuSolver/issues"
  },
  "homepage": "https://github.com/PipaGs/sudokuSolver#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.1",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsdown": "^0.11.0-beta.5",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  }
}
